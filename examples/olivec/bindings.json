{
  "version": 0,
  "from": [
    "../../build/olivec.linux.ini",
    "../../build/olivec.macos.ini",
    "../../build/olivec.windows.ini"
  ],
  "to": {
    "language": "odin",
    "package": "olivec",
    "trim_prefix": {
      "functions": "olivec_",
      "types": "Olivec_",
      "constants": "OLIVEC_"
    },
    "no_build_tag": true,
    "use_when_else": true,
    "ignore_arch": true,
    "out": "olivec/olivec.odin"
  }
}